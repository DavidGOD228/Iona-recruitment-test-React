{
	"compilerOptions": {
		"jsx": "react",
		"skipLibCheck": true,
		"outDir": "dist/",
		"noImplicitAny": false,
		"module": "commonjs",
		"target": "es2015",
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"lib": ["ES5", "ES6", "DOM", "DOM.Iterable", "ESNext"],
		"resolveJsonModule": true,
		"sourceMap": true,
		"inlineSources": true
	},
	"include": ["./src/**/*"],
	"exclude": ["node_modules"],
}